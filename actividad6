import discord
from discord.ext import commands
import random

intents = discord.Intents.default()
intents.message_content = True

bot = commands.Bot(command_prefix='$', intents=intents)

@bot.command()
async def help(ctx):
    await ctx.send

@bot.command()
async def valorant(ctx):
      await ctx.send("Valorant es un juego de disparos gratuito en primera persona de Riot Games, lanzado el 2 de junio de 2020 para Windows tras una beta cerrada en abril. Inspirado en Counter-Strike, adopta mecánicas como el menú de compra y patrones de spray, y próximamente estará disponible en Playstation 5 y Xbox Series X|S.")

@bot.command()
async def lol(ctx):
    await ctx.send("League of Legends (también conocido por sus siglas LoL) es un videojuego multijugador de arena de batalla en línea desarrollado y publicado por Riot Games.")

@bot.command()
async def lol(ctx):
    await ctx.send("")

bot.run("TOKEN")   
